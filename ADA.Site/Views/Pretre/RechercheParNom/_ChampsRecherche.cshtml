@model ADA.Site.Models.RecherchePretreViewModel

<div class="row search-form">
    <div class="col-md-12">
        <div class="form-group row">
            @Html.LabelFor(x => x.Nom, htmlAttributes: new { @class = "control-label col-md-3" })
            <div class="col-md-9">
                @Html.EditorFor(x => x.Nom, new { htmlAttributes = new { @class = "form-control" } })
            </div>
        </div>
        <div class="form-group row">
            @Html.LabelFor(x => x.Prenom, htmlAttributes: new { @class = "control-label col-md-3" })
            <div class="col-md-9">
                @Html.EditorFor(x => x.Prenom, new { htmlAttributes = new { @class = "form-control" } })
            </div>
        </div>
        <div class="form-group row">
            @Html.LabelFor(x => x.Commune, htmlAttributes: new { @class = "control-label col-md-3" })
            <div class="col-md-9">
                @Html.EditorFor(x => x.Commune, new { htmlAttributes = new { @class = "form-control commune" } })
            </div>
        </div>
        <div class="form-group row">
            @Html.LabelFor(x => x.AnneeNaissance, htmlAttributes: new { @class = "control-label col-md-3 " })
            <div class="col-md-9">
                @Html.EditorFor(x => x.AnneeNaissance, new { htmlAttributes = new { @class = "form-control" } })
            </div>
        </div>
        <div class="form-group row">
            @Html.LabelFor(x => x.AnneeDeces, htmlAttributes: new { @class = "control-label col-md-3" })
            <div class="col-md-9">
                @Html.EditorFor(x => x.AnneeDeces, new { htmlAttributes = new { @class = "form-control" } })
            </div>
        </div>
        <button type="submit" class="btn btn-primary">Rechercher</button>
    </div>
</div>

